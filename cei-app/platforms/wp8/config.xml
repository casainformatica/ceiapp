<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="cei.cei_app" version="5.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <feature name="File">
        <param name="wp-package" value="File" />
    </feature>
    <feature name="InAppBrowser">
        <param name="wp-package" value="InAppBrowser" />
    </feature>
    <feature name="NetworkStatus">
        <param name="wp-package" value="NetworkStatus" />
    </feature>
    <icon height="62" src="res/icons/wp8/ApplicationIcon.png" width="62" />
    <icon height="173" src="res/icons/wp8/Background.png" width="173" />
    <splash height="800" src="res/screens/wp8/SplashScreenImage.jpg" width="480" />
    <name>CEI-App</name>
    <description>La app del CEI</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">marianodvazquez@gmail.com</author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="*" />
    <vs:features />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
    <vs:plugin name="org.apache.cordova.file" version="1.3.3" />
    <preference name="Fullscreen" value="True" />
    <preference name="KeepRunning" value="True" />
    <preference name="ShowTitle" value="False" />
    <preference name="InAppBrowserStorageEnabled" value="True" />
    <preference name="SuppressesIncrementalRendering" value="False" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.12" />
    <vs:plugin name="org.apache.cordova.network-information" version="0.2.15" />
    <vs:plugin name="org.apache.cordova.inappbrowser" version="0.6.0" />
    <preference name="Orientation" value="default" />
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget id="CEI-App" platformName="windows" version="1.0.0.0">
            <vs:name>CEI-App</vs:name>
        </vs:platformSpecificWidget>
    </vs:platformSpecificValues>
</widget>
